{
  "name": "@darksoil/gather",
  "description": "Webcomponent lit-element following open-wc recommendations",
  "author": "guillem@darksoil.studio",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w --preserveWatchOutput false",
    "lint": "eslint --ext .ts,.html . --ignore-path .gitignore && prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "format": "eslint --ext .ts,.html . --fix --ignore-path .gitignore && prettier \"**/*.ts\" --write --ignore-path .gitignore"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./dist/*": "./dist/*"
  },
  "dependencies": {
    "@darksoil/assemble": "^0.3.3",
    "@holochain-open-dev/elements": "^0.5.11",
    "@holochain-open-dev/file-storage": "^0.4.1",
    "@holochain-open-dev/profiles": "^0.14.1",
    "@holochain-open-dev/stores": "^0.5.2",
    "@holochain-open-dev/utils": "^0.14.0",
    "@holochain/client": "^0.14.1",
    "@lit-labs/context": "^0.2.0",
    "@lit-labs/task": "^2.0.0",
    "@lit/localize": "^0.11.4",
    "@mdi/js": "^7.2.96",
    "@msgpack/msgpack": "^2.7.2",
    "@scoped-elements/event-calendar": "^0.1.0",
    "@shoelace-style/shoelace": "^2.3.0",
    "lit": "^2.6.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@lit/localize-tools": "^0.6.3",
    "@open-wc/eslint-config": "^2.0.0",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "14.11.1",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "deepmerge": "^3.2.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "prettier": "^2.0.4",
    "tslib": "^2.0.0",
    "typescript": "^4.9.0"
  }
}
